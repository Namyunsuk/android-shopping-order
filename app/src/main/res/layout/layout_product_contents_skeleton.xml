<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.products.ProductContentsActivity">


        <View
            android:id="@+id/iv_product_skeleton_1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/ic_product_skeleton"
            app:layout_constraintDimensionRatio="154:206"
            app:layout_constraintEnd_toStartOf="@id/iv_product_skeleton_2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <View
            android:id="@+id/iv_product_skeleton_2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/ic_product_skeleton"
            app:layout_constraintDimensionRatio="154:206"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_product_skeleton_1"
            app:layout_constraintTop_toTopOf="parent"/>

        <View
            android:id="@+id/iv_product_skeleton_3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/ic_product_skeleton"
            app:layout_constraintDimensionRatio="154:206"
            app:layout_constraintEnd_toStartOf="@id/iv_product_skeleton_4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/iv_product_skeleton_1" />

        <View
            android:id="@+id/iv_product_skeleton_4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/ic_product_skeleton"
            app:layout_constraintDimensionRatio="154:206"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_product_skeleton_3"
            app:layout_constraintTop_toBottomOf="@id/iv_product_skeleton_2" />
        <View
            android:id="@+id/iv_product_skeleton_5"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/ic_product_skeleton"
            app:layout_constraintDimensionRatio="154:206"
            app:layout_constraintEnd_toStartOf="@id/iv_product_skeleton_6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/iv_product_skeleton_3" />

        <View
            android:id="@+id/iv_product_skeleton_6"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/ic_product_skeleton"
            app:layout_constraintDimensionRatio="154:206"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_product_skeleton_5"
            app:layout_constraintTop_toBottomOf="@id/iv_product_skeleton_4" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</layout>
